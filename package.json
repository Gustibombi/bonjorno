{
  "name": "bonjorno",
  "version": "0.1.0",
  "description": "Logseq-style daily notes view with chronological scrolling",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "format": "prettier --write \"src/**/*.ts\" \"*.json\" \"*.md\" \"*.css\""
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "daily-notes",
    "journal",
    "logseq"
  ],
  "author": "Bonjorno Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.8",
    "obsidian": "latest",
    "prettier": "^3.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "packageManager": "pnpm@8.11.0"
}
